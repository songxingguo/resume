<%- require('./layout/header.ejs')({item: edu}) -%>
<div class="flex-row flex-content-center mt30 pl50 pr50 not-for-print only-for-pc">
    <div class="edu-item pr100 flex-row">
        <div class="mr100">
            <ul class="resume-item__text list-style">
                <% edu.list[0].descList.forEach(function (item)  { %>
                    <li><%= item %></li>
                <% }) %>
            </ul>
        </div>
        <img src="<%= edu.list[0].logoUrl %>" alt="<%= edu.list[0].name %>">
    </div>
    <div class="edu-item split-line flex-row pl100">
        <img src="<%= edu.list[1].logoUrl %>" alt="<%= edu.list[0].name %>">
        <div class="ml100">
            <ul class="resume-item__text list-style">
                <% edu.list[1].descList.forEach(function (item)  { %>
                    <li><%= item %></li>
                <% }) %>
            </ul>
        </div>
    </div>
</div>
<div class="flex-column mt30 not-for-print only-for-print show-for-mb">
    <div class="edu-item pb30 flex-row">
        <img src="<%= edu.list[0].logoUrl %>" alt="<%= edu.list[0].name %>">
        <div class="ml30">
            <ul class="resume-item__text list-style">
                <% edu.list[0].descList.forEach(function (item)  { %>
                    <li><%= item %></li>
                <% }) %>
            </ul>
        </div>
    </div>
    <div class="edu-item pt30 flex-row">
        <img src="<%= edu.list[1].logoUrl %>" alt="<%= edu.list[0].name %>">
        <div class="ml30">
            <ul class="resume-item__text list-style">
                <% edu.list[1].descList.forEach(function (item)  { %>
                    <li><%= item %></li>
                <% }) %>
            </ul>
        </div>
    </div>
</div>
<div class="flex-column mt10 only-for-print">
    <% edu.list.forEach(function (item)  { %>
        <div class="flex-row flex-content-between">
            <div class="resume-item__desc"><%= item.major %></div>
            <div class="resume-item__desc"><%= item.term %></div>
            <div class="resume-item__desc"><%= item.name %></div>
        </div>
    <% }) %>
</div>
