<div class="flex-column flex-items-center not-for-print">
    <div class="resume__item-title"><%= tech.title %></div>
    <div class="resume__item-desc"><%= tech.desc %></div>
</div>
<div class="flex-row flex-items-end only-for-print">
    <div class="resume__item-title"><%= tech.title %></div>
    <div class="resume__item-desc ml20"><%= tech.desc %></div>
</div>
<div class="resume__item__body mt20">
    <ul class="ml30 not-for-print not-for-print only-for-pc">
        <% tech.list.forEach(function (item)  { %>
            <li class="tech__item flex-row flex-items-center" style="width: 20%;">
                JavaScript
            </li>
        <% }) %>
    </ul>
    <ul class="resume__item-text list-style only-for-print show-for-mb">
        <% tech.list.forEach(function (item)  { %>
            <li><%= item %></li>
        <% }) %>
    </ul>
</div>
